{{#plans}}
<div class="container-fluid">
    <div class="row d-flex justify-content-center">
        <div class="col-12">
            <div class="card card0 border-0">
                <div class="row">
                    <div class="col-12">
                        <div class="card card00 m-2 border-0">
                            <div class="row text-center justify-content-center">
                                <h3 class="mt-1">{{name}}</h3>
                            </div>
                            <div class="d-flex flex-md-row px-3 mt-4 flex-column-reverse">
                                <div class="col-md-12">
                                    <div class="card1">
                                        <ul id="progressbar" class="text-center">
                                            {{#competencies}}
                                                <li class="{{#isproficient}}active{{/isproficient}} step0"></li>
                                            {{/competencies}}
                                            <li class="step0 {{#iscompleted}}active{{/iscompleted}}"></li>
                                        </ul>
                                        {{#competencies}}
                                        <p>{{competency.shortname}}</p>
                                        {{/competencies}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-12">
                            <div class="row">
                                <h2 class="text-muted final">Completed!</h2>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
{{/plans}}
